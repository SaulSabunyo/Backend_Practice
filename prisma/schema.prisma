generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Student {
  id       Int     @id @default(autoincrement())
  password String  
  name     String  @unique
  contact  String  @unique
  school   School? @relation(fields: [schoolId], references: [id])
  schoolId Int?
}

model School {
  id       Int       @id @default(autoincrement())
  name     String    @unique
  contact  String    @unique
  students Student[]
}
